
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int q;cin>>q; int arr[q],idx=0;
    while(q--){
        int type;   cin>>type;
        if(type==0){
            int ele; cin>>ele;
            arr[idx]=ele; idx++;
        }
        else{
            int L,r,x,y;  cin>>L>>r>>x;
            long long int xoro=0,maxXor=0;
//            if(r > idx-1){
//                r=idx-1;
//            }
            for(int i=L-1;i<=r-1;i++){
                xoro=x^arr[i];
                if(maxXor < xoro){
                    y=arr[i];
                    maxXor=xoro;
                }
            }
            cout<<y<<endl;
        }


    }
}



