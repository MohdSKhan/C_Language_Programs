//how to check 2 pow 0.35
#include<stdio.h>
#include<conio.h>
void main()
{   //int a,b,c,D;
    int i,y;
    float x,ans=1;
    printf("enter x,y:");
    scanf("%f%d",&x,&y);
    if(y>=0)
    {
        for(i=1;i<=y;i++)
            ans*=x;
        printf("ans is %f",ans);

    }
    else
    {
        for(i=1;i<=abs(y);i++)
            ans*=(1/x);
        printf("ans is %f",ans);
    }
}
