#include<stdio.h>
#include<conio.h>
void main()
{
    int i,count=0;
    char s[50];
    printf("enter: ");
    gets(s);
    for(i=0;s[i];i++)
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u' || s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U')
            count++;
    printf("vowels: %d",count);


}


